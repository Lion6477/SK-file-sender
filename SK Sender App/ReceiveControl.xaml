<UserControl x:Class="SK_Sender_App.ReceiveControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:wpf1="clr-namespace:LiveChartsCore.SkiaSharpView.WPF;assembly=LiveChartsCore.SkiaSharpView.WPF">
    <Grid>
        <Button Name="btnBackReceive" Content="Back" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100"
                Height="30" Click="btnBackReceive_Click" IsEnabled="True" />
        <TextBlock Name="txtStatusReceive" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,40,0,0"
                   Width="300" Height="30" Text="Logs will be here..." />
        <wpf1:CartesianChart Name="speedChartReceive" HorizontalAlignment="Left" VerticalAlignment="Top" Width="500"
                             Height="200" Margin="10,80,0,0" />
        <ProgressBar Name="progressBarReceive" HorizontalAlignment="Left" VerticalAlignment="Top" Width="500"
                     Height="20" Margin="10,290,0,0" />
    </Grid>
</UserControl>