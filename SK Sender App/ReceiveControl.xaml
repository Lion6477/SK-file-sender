<UserControl x:Class="SK_Sender_App.ReceiveControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:wpf1="clr-namespace:LiveChartsCore.SkiaSharpView.WPF;assembly=LiveChartsCore.SkiaSharpView.WPF"
             Margin="10">
    <Grid>
        <TextBlock Name="txtStatusReceive" HorizontalAlignment="Left" VerticalAlignment="Top" Width="300" Height="30" />
        <wpf1:CartesianChart Name="speedChartReceive" HorizontalAlignment="Left" VerticalAlignment="Top" Width="500"
                             Height="200" Margin="0,40,0,0" />
        <ProgressBar Name="progressBarReceive" HorizontalAlignment="Left" VerticalAlignment="Top" Width="500"
                     Height="20" Margin="0,260,0,0" />
    </Grid>
</UserControl>